block('map')(
    js()(function() {
      var ctx = this.ctx;
      return {
        id : ctx.id,
        center : ctx.center,
        zoom : ctx.zoom,
        controls : ctx.controls,
        geoObjects : ctx.geoObjects
      };
    }),
    attrs()( function() {
      return { 'id' : this.ctx.id };
    })
);
